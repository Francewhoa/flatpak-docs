msgid "``:ro`` - read-only access"
msgstr ""

#: ../../sandbox-permissions-reference.rst:61
msgid "``:rw`` - read/write access (this is the default)"
msgstr ""

#: ../../sandbox-permissions-reference.rst:62
msgid ""
